#code for setting pics for choice one

SetCoords(pic5, x(AAA), y(AAA))
SetConditionToShowObject( pic5, q = 4 && selected = 1)
SetLayer( pic5, 0 )
SetFixed( pic5, true )
Rename( pic5, "BgChoiceOne4" )

